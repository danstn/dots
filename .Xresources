!-------------------------------------------------------------------------------
! Xft settings
!-------------------------------------------------------------------------------
Xft.rgba: rgb
Xft.antialias: true
Xft.autohint: false
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
!Xft.dpi: 109
!Xft.dpi: 163 
!Xft.dpi: 210
!Xft.dpi: 144
Xcursor.size:16

!-------------------------------------------------------------------------------
! URxvt settings
!-------------------------------------------------------------------------------
URxvt.font: xft:Hack:Inconsolata:Terminus:DejaVuSansMono:size=12:antialias=true
URxvt.boldFont:
URxvt*scrollBar: false
URxvt*transparent: true
URxvt*shading: 20
URxvt*fading: 5
URxvt*depth: 32
URxvt*cursorBlink: true

*background: #111314
*foreground: #B7BBB7
! black
*color0: #2C2F33
*color8: #4B5056
! red
*color1: #B04C50
*color9: #B04C50
! green
*color2: #919652
*color10: #94985B
! yellow
*color3: #E2995C
*color11: #E2995C
! blue
*color4: #66899D
*color12: #66899D
! magenta
*color5: #8D6494
*color13: #8D6494
! cyan
*color6: #527C77
*color14: #527C77
! white
*color7: #606360
*color15: #DDE3DC
